                     cnt++;
                     if (cnt%4 == 0){htmlStr = htmlStr + "</div>"};
                   }
                   catch (e) {$("#alertModal").modal('show')}
                 }) //for each
                 if (cnt%4 != 0) {htmlStr = htmlStr + "</div>"};
                 htmlStr = htmlStr + '<hr></div>';
